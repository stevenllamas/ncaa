    "# merge the original df with df_opp on the game_id.\n",
    "df = pd.merge(df, df_opp, on=\"game_id\", how=\"inner\")\n",
    "\n",
    "# remove  rows where the team is paired with itself.\n",
    "df = df[df[\"team\"] != df[\"opp_team\"]]\n",
    "\n",
    "#  Reset the index\n",
    "df = df.reset_index(drop=True)"
